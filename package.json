{
  "name": "solana-metadata-tool",
  "version": "1.0.0",
  "description": "JavaScript скрипт для добавления метаданных к Solana SPL токенам через Metaplex Token Metadata Protocol",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "mainnet": "SOLANA_NETWORK=mainnet node src/index.js",
    "check": "node scripts/check-config.js",
    "test": "node scripts/test-setup.js"
  },
  "keywords": [
    "solana",
    "spl-token",
    "metaplex",
    "metadata",
    "ipfs",
    "blockchain",
    "cryptocurrency"
  ],
  "author": "eldorkenjabayev13",
  "license": "MIT",
  "dependencies": {
    "@metaplex-foundation/js": "^0.19.4",
    "@metaplex-foundation/mpl-token-metadata": "^2.13.0",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.95.2",
    "dotenv": "^16.6.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
