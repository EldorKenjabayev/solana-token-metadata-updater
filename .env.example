# Пример файла .env - переименуйте в .env и измените значения
# Настройки сети (devnet или mainnet-beta)
SOLANA_NETWORK=devnet

# Адрес вашего токена (замените на свой)
TOKEN_MINT_ADDRESS=YOUR_TOKEN_ADDRESS_HERE

# Метаданные токена
TOKEN_NAME=My Super Token
TOKEN_SYMBOL=MST
# URL вашего логотипа (размер 200x200 рекомендуется)
TOKEN_URI=https://example.com/token-logo.png

# Путь к файлу кошелька
WALLET_PATH=./wallets/keypair.json
